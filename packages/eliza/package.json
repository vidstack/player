{
  "name": "@vidstack/eliza",
  "version": "1.4.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "bin": {
    "eliza": "cli.js"
  },
  "exports": {
    ".": "./dist-node/index.js",
    "./cli.js": "./cli.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:base": "node ../../.scripts/build.js --entry=src/index.ts",
    "build:dist": "pnpm build:base -- --outdir=dist-node --platform=node --bundle --requireshim --external=yargs,@vidstack/foundation,typescript",
    "build:types": "tsc -p tsconfig.json --pretty",
    "clean": "rimraf dist-node types",
    "watch": "run-p \"build:dist -- -w\" \"build:types -- -w\"",
    "lint:eslint": "eslint src --fix",
    "lint:prettier": "prettier src --write --loglevel warn"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lru-cache": "^7.4.0",
    "@types/node": "^17.0.17",
    "@types/normalize-path": "^3.0.0",
    "@types/read-pkg-up": "^6.0.0",
    "@types/yargs": "^17.0.8",
    "@vidstack/foundation": "workspace:*",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "kleur": "^4.1.4",
    "lru-cache": "^7.4.4",
    "normalize-path": "^3.0.0",
    "read-pkg-up": "^9.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
