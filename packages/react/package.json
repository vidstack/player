{
  "name": "@vidstack/react",
  "version": "0.2.0",
  "description": "Build awesome media experiences on the web.",
  "license": "MIT",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "module": "dist/prod.js",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "files": [
    "*.d.ts",
    "dist/"
  ],
  "scripts": {
    "dev": "pnpm clean && tsup --watch & pnpm run types -w",
    "build": "node .scripts/prebuild.js && tsup",
    "types": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier src --write --loglevel warn"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "maverick.js": "^0.32.2",
    "react": "^18.0.0",
    "vidstack": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "maverick.js": "^0.32.2",
    "react": "^18.0.0",
    "rimraf": "^3.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.0",
    "vidstack": "workspace:*"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidstack/vidstack.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/vidstack/vidstack/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/server.cjs",
      "node": "./dist/server.js",
      "deno": "./dist/server.js",
      "development": "./dist/dev.js",
      "default": "./dist/prod.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vidstack",
    "player",
    "react"
  ]
}
